MeshBuilder.CreateSphere('sphere', {segments:16, diameter:200}, scene);\r\n    star.position.y = 1;\r\n    star.material = starMaterial\r\n\r\n    // Return the created scene.\r\n    return scene;\r\n}\r\n"},"lineCount":null}},"hash":"ab98f9c0426460c47c70549015332b0e","cacheData":{"env":{}}}